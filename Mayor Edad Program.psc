Algoritmo Mayor_Edad 
	//Autor: RodolfoA. Martinez;
	//Este Programa se usa para Calcular si la persona es Mayor o Menor de Edad;
	
	_autor <- "Rodolfo A. Martinez";
	Escribir "*****Bienvenidos a este Programa **********";
	Escribir "***Copyright 2019 @ ",_autor "****";
	Escribir " ";
	Escribir "Digite Edad";
	Leer Edad;
	Si Edad>=18 Entonces
		Escribir "Mayor Edad";
	SiNo
		Escribir "Menor Edad";
	FinSi
	Escribir " ";
FinAlgoritmo
